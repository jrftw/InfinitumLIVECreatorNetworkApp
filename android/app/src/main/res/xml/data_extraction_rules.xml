<?xml version="1.0" encoding="utf-8"?>
<!--
  Filename: data_extraction_rules.xml
  Purpose: Data extraction rules for Android 12+ devices
  Author: Kevin Doyle Jr. / Infinitum Imagery LLC
  Last Modified: 2025-01-27
  Platform Compatibility: Android 12+
-->
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive data from cloud backup -->
        <exclude domain="sharedpref" path="flutter." />
    </cloud-backup>
    <device-transfer>
        <!-- Exclude sensitive data from device transfer -->
        <exclude domain="sharedpref" path="flutter." />
    </device-transfer>
</data-extraction-rules>

